{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Simple",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "simple", 
        "file=../datamodel_test2/schema/simple_schema.json"
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "K8s",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "k8s", 
        "dumpschema=k8s_schema.json",
        "nographviz=true",
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Swagger Data",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "json",
        "files=../datamodel_test2/schema/kubernetes_swagger.json",
        "tweaks=../datamodel_test2/schema/swagger_json_tweaks.json",
        "paths-where-key-is-data=.definitions.properties",
        "same-name-is-same-model=true",
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Yaml Data",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "yaml", 
        "paths= ../../tmp/f1.yaml, ../../tmp/f2.yaml, ../../tmp/f3.yaml, ../../tmp/f4.yaml, ../../tmp/f5.yaml",
        "title=yaml"
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "JSON Data",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "json",
        "paths= ../../Downloads",
        "title=JSON",
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Proto",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "proto", 
        "nographviz=true",
        "paths=sample/proto/a.proto",
        "import-root=sample/proto",
        "url-pattern=github.com/bartekmi/datamodel/tree/master/datamodel/sample/proto/$FILE#L$LINE",
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Proto - Adhoc",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "proto", 
        "nographviz=true",
        "paths=/Users/bartekm/tmp/proto",
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "XSD",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/net7.0/datamodel.dll",
      "args": [
        "xsd", 
        "url=https://www.netchb.com/xml/entry/entry.xsd",
        "dumpschema=../../tmp/xsd.out",
        "dropsuffix=Type"
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
  ]
}
