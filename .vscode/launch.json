  "version": "0.2.0",
  "configurations": [
    {
      "name": "Simple",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/netcoreapp3.1/datamodel.dll",
      "args": [
        "simple", 
        "file=../datamodel_test2/schema/simple_schema.json"
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "externalTerminal",
      "stopAtEntry": false
    },
    {
      "name": "K8s",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/datamodel/bin/Debug/netcoreapp3.1/datamodel.dll",
      "args": [
        "k8s", 
      ],
      "cwd": "${workspaceFolder}/datamodel",
      "console": "externalTerminal",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
